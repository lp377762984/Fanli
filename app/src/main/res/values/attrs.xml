<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="MoneyBar">
        <attr name="textTitle" format="string" />
        <attr name="textTail" format="string" />
        <attr name="needBack" format="boolean" />
        <attr name="tailIcon" format="reference|integer" />
        <attr name="needTailIcon" format="boolean" />
        <attr name="needComplete" format="boolean" />
        <attr name="barBg" format="color" />
        <attr name="titleColor" format="color" />
        <attr name="backImg" format="reference|integer" />
        <attr name="needLine" format="boolean" />
    </declare-styleable>
    <declare-styleable name="EditTextWithCustomHint">
        <attr name="hintSize" format="dimension" />
    </declare-styleable>
    <declare-styleable name="ShopABar">
        <attr name="abar_title" format="string|reference" />
        <attr name="abar_icon" format="integer|reference" />
        <attr name="abar_bg" format="color|reference" />
    </declare-styleable>

    <declare-styleable name="ShopBarIndicator">
        <attr name="bar_title" format="string|reference" />
        <attr name="bar_img1" format="integer|reference" />
        <attr name="bar_img1_indicator" format="boolean|reference" />
        <attr name="bar_img2" format="integer|reference" />
        <attr name="bar_img2_indicator" format="boolean|reference" />
    </declare-styleable>

    <declare-styleable name="MsgView">
        <attr name="msg_img" format="integer|reference"></attr>
        <attr name="msg_indicator" format="boolean|reference" />
    </declare-styleable>

    <declare-styleable name="MultiViewPager">
        <attr name="matchChildWidth" format="reference" />
        <attr name="android:maxWidth" />
        <attr name="android:maxHeight" />
    </declare-styleable>

    <declare-styleable name="PtrFrameLayout">
        <attr name="ptr_header" format="reference" />
        <attr name="ptr_content" format="reference" />
        <attr name="ptr_resistance" format="float" />
        <attr name="ptr_ratio_of_header_height_to_refresh" format="float" />
        <attr name="ptr_duration_to_close" format="integer" />
        <attr name="ptr_duration_to_close_header" format="integer" />
        <attr name="ptr_pull_to_fresh" format="boolean" />
        <attr name="ptr_keep_header_when_refresh" format="boolean" />
    </declare-styleable>

    <declare-styleable name="SuspendLayout">
        <attr name="showPercent" format="float" />
    </declare-styleable>

    <attr name="borderRadius" format="dimension" />
    <attr name="type">
        <enum name="circle" value="0" />
        <enum name="round" value="1" />
    </attr>

    <declare-styleable name="RoundImageView">
        <attr name="borderRadius" />
        <attr name="type" />
    </declare-styleable>

    <declare-styleable name="DashView">
        <attr name="dashWidth" format="dimension" />
        <attr name="lineWidth" format="dimension" />
        <attr name="lineHeight" format="dimension" />
        <attr name="lineColor" format="color" />
        <attr name="dashOrientation" format="integer" />

    </declare-styleable>


</resources>
