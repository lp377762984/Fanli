<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/activity_bcprocess"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    tools:context="com.ascba.rebate.activities.me_page.business_center_child.BCProcessActivity">
    <com.ascba.rebate.view.MoneyBar
        android:id="@+id/mb"
        app:textTitle="商户中心"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"/>
    <Button
        android:id="@+id/btn"
        android:onClick="goApply"
        android:text="已了解，前往入驻"
        android:layout_alignParentBottom="true"
        android:layout_marginBottom="30dp"
        android:layout_marginTop="30dp"
        style="@style/btnNormal" />
    <WebView
        android:background="@color/main_content_bg"
        android:layout_below="@id/mb"
        android:layout_above="@id/btn"
        android:id="@+id/webView"
        android:layout_width="match_parent"
        android:layout_height="match_parent"/>
</RelativeLayout>
